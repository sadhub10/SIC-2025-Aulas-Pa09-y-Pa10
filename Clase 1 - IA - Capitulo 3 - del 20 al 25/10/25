        "\n",
        "print(\"Tipo de lista:\", type(lista_heterogenea))\n",
        "print(\"Contenido de la lista:\", lista_heterogenea)\n",
        "print(\"Tipo del primer elemento:\", type(lista_heterogenea[0]))\n",
        "print(\"Tipo del segundo elemento:\", type(lista_heterogenea[1]))"
