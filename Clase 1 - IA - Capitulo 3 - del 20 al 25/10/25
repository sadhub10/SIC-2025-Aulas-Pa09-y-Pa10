        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "import seaborn as sns # Opcional: mejora la estética de los gráficos\n",
        "\n",
        "# Configuración básica de seaborn para mejor estética (opcional)\n",
        "sns.set_theme(style=\"whitegrid\")\n",
        "\n",
        "# Datos de ejemplo: puntajes de examen (variable continua)\n",
        "np.random.seed(42) # Para reproducibilidad\n",
        "puntajes = np.random.normal(loc=70, scale=10, size=200) # Distribución normal con media 70, desviación 10\n",
        "puntajes = np.clip(puntajes, 0, 100) # Asegurar que los puntajes estén entre 0 y 100\n",
        "df_puntajes = pd.DataFrame({'Puntaje_Examen': puntajes})\n",
        "\n",
        "print(\"\\n--- DataFrame para Histograma ---\")\n",
        "print(df_puntajes.head()) # Mostrar solo las primeras filas\n",
        "\n",
        "# Crear el histograma directamente desde la columna del DataFrame\n",
        "plt.figure(figsize=(8, 5)) # Tamaño del gráfico\n",
        "df_puntajes['Puntaje_Examen'].hist(bins=10, edgecolor='black', alpha=0.7) # bins define el número de cubetas\n",
        "\n",
        "plt.title('Distribución de Puntajes de Examen')\n",
        "plt.xlabel('Puntaje de Examen')\n",
        "plt.ylabel('Frecuencia')\n",
        "plt.grid(axis='y', alpha=0.75) # Mostrar cuadrícula en el eje Y\n",
        "plt.tight_layout() # Ajustar el diseño\n",
        "plt.show() # Mostrar el gráfico"
