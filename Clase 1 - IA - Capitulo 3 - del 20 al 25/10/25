        "\n",
        "# Array de NumPy (idealmente con el mismo tipo de datos)\n",
        "# Nota: Si se mezclan tipos, NumPy intentará convertir todos al tipo más general.\n",
        "array_homogeneo = np.array([10, 20, 30, 40])\n",
        "\n",
        "print(\"Tipo de array:\", type(array_homogeneo))\n",
        "print(\"Contenido del array:\", array_homogeneo)\n",
