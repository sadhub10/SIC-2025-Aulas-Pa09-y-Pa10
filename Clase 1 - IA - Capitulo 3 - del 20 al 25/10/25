    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns # Opcional: mejora la estética de los gráficos\n",
        "\n",
        "# Configuración básica de seaborn para mejor estética (opcional)\n",
        "sns.set_theme(style=\"whitegrid\")\n",
        "\n",
        "# Datos de ejemplo: número de estudiantes por curso (categórica)\n",
        "datos_cursos = {\n",
        "    'Curso': ['Matemáticas', 'Historia', 'Ciencias', 'Literatura'],\n",
        "    'Num_Estudiantes': [45, 30, 60, 25]\n",
        "}\n",
        "df_cursos = pd.DataFrame(datos_cursos)\n",
        "\n",
        "print(\"--- DataFrame para Gráfico de Barras ---\")\n",
        "print(df_cursos)\n",
        "\n",
        "# Crear el gráfico de barras directamente desde el DataFrame\n",
        "plt.figure(figsize=(8, 5)) # Tamaño del gráfico\n",
        "df_cursos.plot(kind='bar', x='Curso', y='Num_Estudiantes', legend=False)\n",
        "\n",
        "plt.title('Número de Estudiantes por Curso')\n",
        "plt.xlabel('Curso')\n",
        "plt.ylabel('Número de Estudiantes')\n",
        "plt.xticks(rotation=45, ha='right') # Rotar etiquetas del eje X para mejor lectura\n",
        "plt.tight_layout() # Ajustar el diseño para que no se superpongan los elementos\n",
        "plt.show() # Mostrar el gráfico"
      ],
      "metadata": {
