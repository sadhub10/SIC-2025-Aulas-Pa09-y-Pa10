    {
      "cell_type": "code",
      "source": [
        "def triangulo_de_pascal_centrado(num_filas):\n",
        "    \"\"\"\n",
        "    Genera el Tri√°ngulo de Pascal centrado.\n",
        "\n",
        "    Args:\n",
        "        num_filas (int): El n√∫mero de filas a generar.\n",
        "    \"\"\"\n",
        "    if num_filas <= 0:\n",
        "        return []\n",
        "\n",
        "    triangulo = [[1]]\n",
        "\n",
        "    for i in range(1, num_filas):\n",
        "        fila_anterior = triangulo[-1]\n",
        "        nueva_fila = [1]\n",
        "        for j in range(len(fila_anterior) - 1):\n",
        "            siguiente_numero = fila_anterior[j] + fila_anterior[j+1]\n",
        "            nueva_fila.append(siguiente_numero)\n",
        "        nueva_fila.append(1)\n",
        "        triangulo.append(nueva_fila)\n",
        "\n",
        "    # Convertir los n√∫meros a strings para calcular el ancho m√°ximo\n",
        "    str_triangulo = [[str(num) for num in fila] for fila in triangulo]\n",
        "\n",
        "    # Calcular el ancho total de la base para centrar las filas\n",
        "    ancho_max_base = len(\" \".join(str_triangulo[-1]))\n",
        "\n",
        "    # Imprimir el tri√°ngulo centrado\n",
        "    for fila in str_triangulo:\n",
        "        fila_str = \" \".join(fila)\n",
        "        print(fila_str.center(ancho_max_base))\n",
        "\n",
        "print(\"Tri√°ngulo de Pascal para 6 filas (Centrado):\")\n",
        "triangulo_de_pascal_centrado(60)"
      ],
